{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "airbnb-base",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "2018",
		"sourceType": "module"
	},
	"rules": {
		"no-undef": 0,
		"no-restricted-syntax": [
			"error",
			"LabeledStatement",
			"WithStatement"
		],
		"linebreak-style": 0,
		"no-tabs": [
			"error",
			{
				"allowIndentationTabs": true
			}
		],
		"indent": [
			"error",
			2
		]
	}
}